{
	"manifest_version": 2,

	"name": "Burkini to Baggy Wetsuit",
    "short_name": "Baggy Wetsuit",
	"description": "Replaces the text 'Burkini' with 'Baggy Wetsuit'.",
	"author": "Alex Chase",
	"version": "1",

    "icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
